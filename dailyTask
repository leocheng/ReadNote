              public void onShow(boolean fromResume) {
                if (fromResume) {
                    mWorkspace.setCurrentPage(1);
                    return;
                }
                //Added by Shuangli.liu for Smart News Panel custom --begin.
                String appName = null;
                try {
                    appName = Settings.System.getString(getContentResolver(),  "app_select_name");
                    if (appName == null || !appName.contains("/")) {
                        startSearch(null, false, null, true);
                    } else {
                        Intent intent = new Intent(Intent.ACTION_MAIN, null);
                        intent.addFlags(Intent.FLAG_ACTIVITY_NEW_TASK);
                        intent.addFlags(Intent.FLAG_ACTIVITY_CLEAR_TASK);
                        intent.addCategory(Intent.CATEGORY_LAUNCHER);
                        intent.setClassName(appName.split("/")[0], appName.split("/")[1]);
                        Launcher.this.startActivity(intent);
                    }
                 } catch (ActivityNotFoundException e) {
                    Log.e(TAG, "Swip to left, could not find custom app:"+appName);
                 }
                //Added by Shuangli.liu for Smart News Panel custom --end.
             }
  
