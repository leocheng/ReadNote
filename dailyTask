            public void sendBroadCast(Activity a, boolean enabled) {
                Intent in = new Intent();
                in.setAction(ACTION_GUESTMODE_STATUS_CHANGE);
                in.putExtra(KEY_ENABLE, enabled);
                a.sendBroadcast(in);
            }

