		    public static void setTextSpan(TextView textView, String text) {
			SpannableString ss = new SpannableString(text);
			ss.setSpan(new TypefaceSpan("roboto"), 0, text.length(),
				Spanned.SPAN_EXCLUSIVE_EXCLUSIVE);
			textView.setText(ss);
		    }


