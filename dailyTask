KeyguardService.java
    KeyguardViewMediator.java
        KeyguardViewManager.java
        
        
        
System start
    --> KeyguardViewMediator.onSystemReady()
        --> doKeyguardLocked() 这个方法来做启动锁屏界面的预处理
            --> showLocked()
                --> handleShow(Bundle options)  // 播放声音
private void handleShow(Bundle options) {
    mKeyguardViewManager.show(options);
    
    mShowing = true;
    mKeyguardDonePending = false;
    updateActivityLockScreenState();
    adjustStatusBarLocked();
    userActivity();
    
    playSounds(true);

    if (mKeyguardDisplayManager != null) {
        mKeyguardDisplayManager.show();
    }
}
    
KeyguardManager.java
    --> show()
public synchronized void show(Bundle options) {
    boolean enableScreenRotation = KeyguardUtils.shouldEnableScreenRotation(mContext);
    KeyguardUpdateMonitor.getInstance(mContext).setQueryBaseTime();
    // 真正添加View的方法
    maybeCreateKeyguardLocked(enableScreenRotation, false, options);
    
    maybeEnableScreenRotation(enableScreenRotation);
    mKeyguardHost.setSystemUiVisibility(visFlags);
    mViewManager.updateViewLayout(mKeyguardHost, mWindowLayoutParams);
    mKeyguardHost.setVisibility(View.VISIBLE);

    mKeyguardView.show();
    mKeyguardView.requestFocus();
    
    if (mKeyguardView.isJrdLockScreenShowing()) {
    	updateShowWallpaper(mLockPatternUtils.isSecure()||mKeyguardView.getSecurity());
    }
}

    private void maybeCreateKeyguardLocked(boolean enableScreenRotation, boolean force,
            Bundle options) {
            // 内部类ViewManagerHost，真正的rootiew，用来挡在屏幕的上方
            mKeyguardHost = new ViewManagerHost(mContext);

            if (!KeyguardViewMediator.isKeyguardInActivity) {
                mViewManager.addView(mKeyguardHost, lp);
            }
            KeyguardUpdateMonitor.getInstance(mContext).registerCallback(mBackgroundChanger);
        }
        
        if (force || mKeyguardView == null) {
            mKeyguardHost.setCustomBackground(null);
            mKeyguardHost.removeAllViews();
            // 加载布局
            inflateKeyguardView(options);
            mKeyguardView.requestFocus();
        }
	
        updateUserActivityTimeoutInWindowLayoutParams();
        mViewManager.updateViewLayout(mKeyguardHost, mWindowLayoutParams);

        mKeyguardHost.restoreHierarchyState(mStateContainer);
    }
在rootView  -- ViewManagerHost中监听configure变化
protected void onConfigurationChanged(Configuration newConfig) {
    --> maybeCreateKeyguardLocked(KeyguardUtils.shouldEnableScreenRotation(mContext), true, null);
}


